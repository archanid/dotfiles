# For mac os Sierra
Host *
  UseKeychain yes
  AddKeysToAgent yes
  #IdentityFile ~/.ssh/id_rsa
  #IdentitiesOnly yes

Host github.com
  HostName github.com
  User archanid
  PreferredAuthentications publickey
  IdentityFile ~/.ssh/id_rsa
  IdentitiesOnly yes
# artistic.archana@gmail.com

Host personal.github.com
  HostName github.com
  User solostyle
  PreferredAuthentications publickey
  IdentityFile ~/.ssh/id_personal_rsa
  IdentitiesOnly yes
# creativehandle@gmail.com

Host go.github.com
  HostName github.com
  User 12go
  PreferredAuthentications publickey
  IdentityFile ~/.ssh/id_go_rsa
  IdentitiesOnly yes
