devDir="$HOME/Development"
alias dev='cd $devDir'
alias dd='tmux new -s work'
alias work='cd $devDir/cloud-nuke'

alias run='BROWSER=Google\ Chrome make run'
alias typecheck='yarn tsc --noEmit --preserveWatchOutput'
alias compile='yarn tsc --diagnostics --noEmit'

alias youtube-mp3='youtube-dl --extract-audio --audio-format mp3'

alias dockerstop='docker stop --time 0 `docker ps --format "{{.ID}}" --last 1`'

alias tor_proxy='docker run -d --rm --name tor -p 8123:8123 -p 9050:9050 thorn-tor-proxy && echo "Tor proxy running at http://localhost:8123 and via SOCKS at 9050"'

alias cerebro='docker run --rm -d -p 9090:9000 yannart/cerebro'

alias docker_reclaim='docker rm $(docker ps -q -f "status=exited") && docker rmi $(docker images -q -f "dangling=true")'

alias awsauth='eval $(pass aws-env-vars)'
alias awsauthmfa='eval $(pass aws-env-vars-mfa)'
