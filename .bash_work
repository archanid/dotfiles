devDir="$HOME/Development"
alias dev='cd $devDir'
alias dd='tmux new -s work'
alias work='cd $devDir/thorn/solis'
export GOOGLE_GEOCODING_KEY=AIzaSyDgSOfomlslY9ezEETOJaBNjB1Bge6HYBw

alias run='BROWSER=Google\ Chrome make run'
alias typecheck='yarn tsc --noEmit --preserveWatchOutput'
alias compile='yarn tsc --diagnostics --noEmit'

alias youtube-mp3='youtube-dl --extract-audio --audio-format mp3'

alias dockerstop='docker stop --time 0 `docker ps --format "{{.ID}}" --last 1`'

alias tor_proxy='docker run -d --rm --name tor -p 8123:8123 -p 9050:9050 thorn-tor-proxy && echo "Tor proxy running at http://localhost:8123 and via SOCKS at 9050"'

alias cerebro='docker run --rm -d -p 9090:9000 yannart/cerebro'

alias docker_reclaim='docker rm $(docker ps -q -f "status=exited") && docker rmi $(docker images -q -f "dangling=true")'

alias oplogin='eval $(~/oplogin)'
alias prod='~/opget solis-prod es'
alias stag='~/opget solis-staging es'
alias stag-admin='~/opget solis-staging-admin es'

alias aws_prod='run_as_aws aws-thorn-production'
alias aws_staging='run_as_aws aws-thorn-staging'
alias aws_solis_prod='run_as_aws aws-thorn-solis-production'
alias aws_solis_staging='run_as_aws aws-thorn-solis-development'

alias k='aws_prod kubectl'
alias kdash='aws_prod kubedash'
alias dockerlogin='$(run_as_aws aws-thorn-production aws ecr get-login --no-include-email --region us-east-1)'
